{
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "functions": [{
        "source": "functions",
        "codebase": "default",
        "ignore": [
            "venv",
            ".git",
            "firebase-debug.log",
            "firebase-debug.*.log"
        ]
    }],
    "hosting": {
        "public": "webapp/built",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "frameworksBackend": {
            "region": "us-central1"
        }
    },
    "storage": {
        "rules": "storage.rules"
    }
}